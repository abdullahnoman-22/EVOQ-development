    </div>
    
    <!-- Footer Bottom -->
    <div class="cus-f-footer-bottom">
      <div class="cus-f-footer-tagline">
        {{ section.settings.tagline | default: 'Premium wellness, reimagined' }}
      </div>
      <!-- Mobile Newsletter Button -->
      <button class="cus-f-mobile-newsletter-btn" onclick="openNewsletterModal()">
        Join our newsletter
      </button>
      {% if section.settings.show_reviews and section.settings.image_review %}
        <div class="cus-f-footer-reviews">
           <img src="{{ section.settings.image_review | img_url: 'master' }}" 
               alt="Customer Reviews" 
               class="cus-f-review-image"
               loading="lazy">
        </div>
      {% endif %}
    </div>
    
    <!-- Footer Meta -->
    <div class="cus-f-footer-meta">
      <div class="cus-f-copyright">
        <p>Â© {{ 'now' | date: '%Y' }} {{ section.settings.company_name | default: 'Feel Holdings Limited' }}. All rights reserved.</p>
        <p>{{ section.settings.address | default: 'Uncommon Holborn, 91-97 High Holborn, London, WC1V 6DF, UK' }}</p>
      </div>
      
      <div class="cus-f-footer-links">
        <a href="{{ section.settings.privacy_policy_url | default: '/policies/privacy-policy' }}">Privacy policy</a>
        <a href="{{ section.settings.shipping_returns_url | default: '/policies/shipping-policy' }}">Shipping and returns</a>
