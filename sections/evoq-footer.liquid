      <!-- Shop Column -->
      <div class="cus-f-footer-column">
        <h3>{{ section.settings.shop_heading | default: 'Shop' }}</h3>
        <ul>
          {% for block in section.blocks %}
            {% if block.type == 'shop_link' %}
              <li>
                <a href="{{ block.settings.link }}">{{ block.settings.label }}</a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <!-- About Column -->
      <div class="cus-f-footer-column">
        <h3>{{ section.settings.about_heading | default: 'About' }}</h3>
        <ul>
          {% for block in section.blocks %}
            {% if block.type == 'about_link' %}
              <li>
                <a href="{{ block.settings.link }}">{{ block.settings.label }}</a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <!-- What is Collagen Column -->
      <div class="cus-f-footer-column">
        <h3>{{ section.settings.collagen_heading | default: 'What is Collagen?' }}</h3>
        <ul>
          {% for block in section.blocks %}
            {% if block.type == 'collagen_link' %}
              <li>
                <a href="{{ block.settings.link }}">{{ block.settings.label }}</a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <!-- Reviews Column -->
      <div class="cus-f-footer-column">
        <h3>{{ section.settings.reviews_heading | default: 'Reviews' }}</h3>
        <ul>
          {% for block in section.blocks %}
            {% if block.type == 'reviews_link' %}
              <li>
                <a href="{{ block.settings.link }}">{{ block.settings.label }}</a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <!-- Blog Column -->
      <div class="cus-f-footer-column">
        <h3>{{ section.settings.blog_heading | default: 'Blog' }}</h3>
        <ul>
          {% for block in section.blocks %}
            {% if block.type == 'blog_link' %}
              <li>
                <a href="{{ block.settings.link }}">{{ block.settings.label }}</a>
              </li>
            {% endif %}
          {% endfor %}
        </ul>
