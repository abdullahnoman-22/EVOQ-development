<style>
  .cus-f-footer-evoq {
    background-color: #001E5F;
    color: white;
    padding: 4rem 0 2rem;
  }
  
  .cus-f-container {
    margin: 0 auto;
  }
  
  .cus-f-footer-content {
    display: grid;
    grid-template-columns: 2fr 1fr 1fr 1fr 1fr 1fr;
    gap: 3rem;
    margin-bottom: 3rem;
    justify-items: center;
  }
  
  .cus-f-footer-brand {
    grid-column: 1 / 2;
    width: 100%;
  }
  
  .cus-f-footer-brand h2 {
    font-size: 48px;
    font-weight: bold;
    margin-bottom: 1.5rem;
    letter-spacing: 0.1em;
    color: #fff;
    margin-top: 0;
  }
  
  .cus-f-newsletter-form {
    display: flex;
    flex-direction: column;
    gap: 1rem;
    max-width: 300px;
  }
  
  .cus-f-newsletter-form input {
    padding: 0.75rem;
    border: 1px solid rgba(255, 255, 255, 0.3);
    background: rgba(255, 255, 255, 0.1);
    color: white;
    border-radius: 4px;
    width: 100%;
  }
  
  .cus-f-newsletter-form input::placeholder {
    color: rgba(255, 255, 255, 0.7);
  }
  
  .cus-f-newsletter-form button {
    padding: 0.75rem 1.5rem;
    background: white;
    color: #1e3a8a;
    border: none;
    border-radius: 4px;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.3s ease;
    width: 50%;
  }
  
  .cus-f-newsletter-form button:hover {
    background: rgba(255, 255, 255, 0.9);
    transform: translateY(-1px);
  }
  
  /* Mobile Newsletter Button - Hidden by default */
  .cus-f-mobile-newsletter-btn {
    display: none;
    width: 100%;
    padding: 1rem 2rem;
    color: #fff;
    border: 2px solid white;
    border-radius: 8px;
    font-weight: 400;
    font-size: 14px;
    cursor: pointer;
    transition: all 0.3s ease;
    text-align: center;
    background: none;
    line-height: 20px;
  }
  
  .cus-f-mobile-newsletter-btn:hover {
    background: transparent;
    color: white;
  }
  
  .cus-f-footer-column h3 {
    font-size: 1.2rem;
    font-weight: 600;
    margin-bottom: 1rem;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    color: #fff;
  }
  
  .cus-f-footer-column ul {
    list-style: none;
    padding: 0;
  }
  
  .cus-f-footer-column li {
    margin-bottom: 0.5rem;
  }
  
  .cus-f-footer-column a {
    color: rgba(255, 255, 255, 0.8);
    text-decoration: none;
    transition: color 0.3s ease;
    font-size: 14px;
  }
  
  .cus-f-footer-column a:hover {
    color: white;
  }
  
  .cus-f-footer-bottom {
    border-top: 1px solid rgba(255, 255, 255, 0.2);
    padding-top: 2rem;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
    gap: 1rem;
  }
  
  .cus-f-footer-tagline {
    font-size: 20px;
    font-weight: 600;
    line-height: 30px;
  }
  
  .cus-f-footer-reviews {
    background: white;
    color: #333;
    padding: 1rem 1.5rem;
    border-radius: 8px;
    display: flex;
    align-items: center;
    gap: 1rem;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    width: 40%;
  }
  
  .cus-f-review-image {
    height: auto;
    border-radius: 4px;
    width: 100%;
  }
  
  .cus-f-review-logos {
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }
  
  .cus-f-footer-meta {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
    margin-top: 1rem;
    flex-wrap: wrap;
    gap: 1rem;
  }
  
  .cus-f-footer-links {
    display: flex;
    gap: 1.5rem;
  }
  
  .cus-f-footer-links a {
    color: rgba(255, 255, 255, 0.7);
    text-decoration: none;
    font-size: 12px;
  }
  
  .cus-f-payment-icons img {
    height: 30px;
    width: 100%;
  }
  
  .cus-f-copyright {
    color: rgba(255, 255, 255, 0.7);
    font-size: 1.2rem;
  }
  
  .cus-f-copyright p {
    margin: 0;
  }

  /* Subscription Success Popup Styles */
  .subscription-popup-overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 9999;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
  }

  .subscription-popup-overlay.show {
    opacity: 1;
    visibility: visible;
  }

  .subscription-popup {
    border-radius: 20px;
    display: flex;
    max-width: 800px;
    width: 90%;
    max-height: 800px;
    overflow: hidden;
    transform: scale(0.7);
    transition: transform 0.3s ease;
    box-shadow: 0 20px 40px rgba(0, 0, 0, 0.2);
    background: #fff;
  }

  .subscription-popup-overlay.show .subscription-popup {
    transform: scale(1);
  }

  .popup-image-section {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 2rem;
    position: relative;
  }

  .popup-product-image {
    width: 400px;
    height: auto;
    filter: drop-shadow(0 10px 20px rgba(0, 0, 0, 0.2));
  }

  .popup-content-section {
    flex: 1;
    background: white;
    padding: 2rem;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    position: relative;
  }

  .popup-close-btn {
    position: absolute;
    top: 1rem;
    right: 1rem;
    background: none;
    border: none;
    font-size: 24px;
    color: #666;
    cursor: pointer;
    width: 30px;
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    transition: all 0.2s ease;
  }

  .popup-close-btn:hover {
    background: #f0f0f0;
    color: #333;
  }

  .popup-title {
    font-size: 28px;
    font-weight: bold;
    color: #333;
    margin: 0 0 0.5rem 0;
    line-height: 1.2;
  }

  .popup-subtitle {
    font-size: 28px;
    font-weight: bold;
    color: #333;
    margin: 0 0 1.5rem 0;
    line-height: 1.2;
  }

  .popup-description {
    font-size: 16px;
    color: #666;
    margin-bottom: 1rem;
    line-height: 1.4;
  }

  .popup-discount-code {
    background: #f8f9fa;
    border: 2px dashed #ddd;
    padding: 0.75rem 1.5rem;
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    margin-bottom: 1rem;
  }

  .discount-code {
    font-size: 18px;
    font-weight: bold;
    color: #333;
    letter-spacing: 1px;
  }

  .copy-btn {
    background: none;
    border: none;
    color: #666;
    font-size: 12px;
    cursor: pointer;
    padding: 0.25rem 0.5rem;
    border-radius: 4px;
    transition: all 0.2s ease;
    text-transform: uppercase;
    font-weight: 600;
  }

  .copy-btn:hover {
    background: #e9ecef;
    color: #333;
  }

  .copy-btn.copied {
    color: #28a745;
  }
  
  @media (max-width: 768px) {
    .cus-f-footer-content {
      grid-template-columns: 1fr 1fr;
      gap: 2rem;
      justify-items: start;
    }
    .cus-f-newsletter-form {
    max-width: unset;
  }
    .cus-f-footer-brand {
      grid-column: 1 / -1;
    }
    
    .cus-f-mobile-newsletter-btn {
      display: block;
    }
    
    .cus-f-footer-bottom {
      flex-direction: column;
      text-align: center;
    }
    
    .cus-f-footer-links a {
      font-size: 11px;
    }
    
    .cus-f-footer-tagline {
      font-size: 16px;
      line-height: 24px;
    }
    
    .cus-f-footer-meta {
      flex-direction: column;
      align-items: center;
    }
    
    .cus-f-footer-links {
      flex-wrap: wrap;
      justify-content: center;
    }
    
    .cus-f-footer-reviews {
      width: 100%;
    }
    
    .cus-f-copyright {
      color: rgba(255, 255, 255, 0.7);
      font-size: 1.2rem;
      order: 3;
    }

    .subscription-popup {
      flex-direction: column;
      max-width: 320px;
      max-height: 90vh;
    }

    .popup-image-section {
      padding: 1.5rem;
    }

    .popup-product-image {
      width: 120px;
    }

    .popup-content-section {
      padding: 1.5rem;
    }

    .popup-title,
    .popup-subtitle {
      font-size: 24px;
    }

    .popup-description {
      font-size: 14px;
    }
  }
</style>

<footer class="cus-f-footer-evoq">
  <div class="page-width cus-f-container">
    
    <div class="cus-f-footer-content">
      <!-- Brand and Newsletter -->
      <div class="cus-f-footer-brand">
        <h2>{{ section.settings.brand_name | default: 'EVOQ' }}</h2>
        
        {% if section.settings.newsletter_enable %}
          <form class="cus-f-newsletter-form" onsubmit="handleNewsletterSubmit(event)">
            <input 
              type="email" 
              name="contact[email]" 
              placeholder="{{ section.settings.newsletter_placeholder | default: 'Your Email' }}" 
              required
            >
            <button type="submit">
              {{ section.settings.newsletter_button_text | default: 'Subscribe' }}
            </button>
          </form>
        {% endif %}
